page {
  background-color: #F7F8FA;
}
view.index {
  padding-top: 30rpx;
}
view.index .swiper {
  margin: 0px, auto;
}
view.index .swiper swiper {
  background-color: white;
  box-shadow: 0px 5px 12px #ebedf0;
  width: 93%;
  margin: 0px auto;
  text-align: center;
  height: 400rpx;
  border-radius: 15rpx;
}
view.index .swiper swiper swiper-item {
  height: 350rpx;
  border-radius: 15rpx;
}
view.index .swiper swiper swiper-item image {
  height: 100%;
  width: 100%;
  border-radius: 15rpx;
}
view.index .searchBackground {
  background-color: white;
  width: 102%;
  margin: 0px -20rpx;
  margin-top: 15rpx;
}
view.index .searchBackground image {
  padding-left: 20rpx;
}
view.index .searchBar {
  background-color: rgba(228, 229, 229, 0.9);
  box-shadow: 0px 5px 12px #ebedf0;
  height: 75rpx;
  border-radius: 15rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 300;
  font-size: 23rpx;
}
view.index .searchCell {
  font-size: 13px;
}
view.index .searchText {
  padding-left: 30rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 300;
  font-size: 23rpx;
}
view.index .searchCellGroup {
  margin-top: -5px;
}
view.index view.categorySelector {
  display: flex;
  margin: 0px auto;
  margin-top: 20rpx;
  justify-content: space-between;
  align-items: center;
}
view.index view.categorySelector .column {
  flex: 20%;
  text-align: center;
}
view.index view.categorySelector .column .icon image {
  width: 110rpx;
  height: 110rpx;
}
view.index view.categorySelector .column .text .title {
  font-family: PingFangSC-Semibold;
  font-size: 30rpx;
  color: #363938;
}
view.index view.categorySelector .column .text .subTitle {
  margin-top: 5rpx;
  font-family: PingFangSC-Regular;
  font-size: 19rpx;
  color: #9F9F9F;
}
view.index view.body {
  padding-bottom: 30rpx;
}
view.index view.body view.row {
  width: 93%;
  margin: 0px auto;
  margin-top: 40rpx;
  border-radius: 15rpx;
  box-shadow: 0px 5px 12px #ebedf0;
  background-color: white;
}
view.index view.body view.row view.background {
  border-radius: 15rpx 15rpx 0rpx 0rpx;
  height: 250rpx;
}
view.index view.body view.row view.background image {
  border-radius: 15rpx;
  height: 440rpx;
  width: 100%;
}
view.index view.body view.row view.information {
  box-shadow: 0px 0px 3px 3px rgba(255, 255, 255, 0.6);
  background-image: linear-gradient(180deg, rgba(255, 255, 255, 0.55) 0%, #FAFCFC 82%);
  backdrop-filter: blur(5px);
  height: 150rpx;
  margin: 0px auto;
  border-radius: 15rpx;
  display: flex;
  align-items: center;
  padding: 10px 7px 12px 0px;
}
view.index view.body view.row view.information view.left {
  flex: 100%;
  padding-left: 10rpx;
  padding-right: 10rpx;
  text-align: left;
  display: flex;
}
view.index view.body view.row view.information view.left view.leaderName {
  font-family: PingFangSC;
  font-size: 26rpx;
  color: #656565;
}
view.index view.body view.row view.information view.left view.storeName {
  margin-top: 6rpx;
  font-family: PingFangSC-Semibold;
  font-size: 40rpx;
  color: #363938;
}
view.index view.body view.row view.information view.left view.description {
  margin-top: 8rpx;
  font-family: PingFangSC-Thin;
  font-size: 23rpx;
  color: #656565;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
view.index view.body view.row view.information view.left image {
  margin-top: 8rpx;
  margin-left: 10rpx;
  margin-right: 20rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
view.index view.body view.row view.information view.right {
  flex: 20%;
  padding-left: 5rpx;
}
view.index view.body view.row view.information view.right .circle {
  margin-left: 10rpx;
  height: 95rpx;
  width: 95rpx;
  text-align: center;
  background: #369472;
  opacity: 0.8;
  border-radius: 30%;
}
view.index view.body view.row view.information view.right .circle image {
  margin-top: 15rpx;
  width: 35rpx;
  height: 35rpx;
}
view.index view.body view.row view.information view.right .circle view.priceTag {
  margin-top: -3px;
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  color: #FAFCFC;
}
view.index view.body button::after {
  border: none;
}
